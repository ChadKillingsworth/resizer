	<metadata>
		<id>ImageResizer.Plugins.AnimatedGifs</id>
		<title>ImageResizer.Plugins.AnimatedGifs - Resize and process animated GIFs</title>
		<version>$version$</version>
		<authors>$author$</authors>
		<owners>$author$</owners>
		<licenseUrl>http://imageresizing.net/licenses/trial</licenseUrl>
